Railways.o : Railways.cpp Railways.h
	g++ -c -std=c++17 -g -O0 -Wall Railways.cpp

BookingClass.o : BookingClass.cpp BookingClass.h
	g++ -c -std=c++17 -g -O0 -Wall BookingClass.cpp
	
BookingCategory.o : BookingCategory.cpp BookingCategory.h
	g++ -c -std=c++17 -g -O0 -Wall BookingCategory.cpp
	
