	g++ -c -std=c++17 -g -O0 -Wall Bad_Mobile.cpp
	
Bad_Name.o : Bad_Name.cpp Bad_Name.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Name.cpp

Bad_Passenger.o : Bad_Passenger.cpp Bad_Passenger.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Passenger.cpp
	
Bad_Railways.o : Bad_Railways.cpp Bad_Railways.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Railways.cpp
	
Bad_Station.o : Bad_Station.cpp Bad_Station.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Station.cpp

Concessions.o : Concessions.cpp Concessions.h
	g++ -c -std=c++17 -g -O0 -Wall Concessions.cpp
	
General_Concession.o : General_Concession.cpp General_Concession.h
	g++ -c -std=c++17 -g -O0 -Wall General_Concession.cpp
	
