# Makefile

# One single executable

a.out : main.o Application_Test.o Bad_Aadhaar.o Bad_Booking.o Bad_Date.o Bad_Mobile.o Bad_Name.o Bad_Passenger.o Bad_Railways.o Bad_Station.o Concessions.o General_Concession.o Ladies_Concession.o SeniorCitizen_Concession.o Divyaang_Concession.o Date.o Station.o Name.o Gender.o Divyaang.o Passenger.o Railways.o BookingClass.o BookingCategory.o Booking.o
	g++ main.o Application_Test.o Bad_Aadhaar.o Bad_Booking.o Bad_Date.o Bad_Mobile.o Bad_Name.o Bad_Passenger.o Bad_Railways.o Bad_Station.o Concessions.o General_Concession.o Ladies_Concession.o SeniorCitizen_Concession.o Divyaang_Concession.o Date.o Station.o Name.o Gender.o Divyaang.o Passenger.o Railways.o BookingClass.o BookingCategory.o Booking.o -o a.out
	
main.o : main.cpp 
	g++ -c -std=c++17 -g -O0 -Wall main.cpp
	
Application_Test.o : Application_Test.cpp 
	g++ -c -std=c++17 -g -O0 Application_Test.cpp

Bad_Aadhaar.o : Bad_Aadhaar.cpp Bad_Aadhaar.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Aadhaar.cpp
	
Bad_Booking.o : Bad_Booking.cpp Bad_Booking.h
	g++ -c -std=c++17 -g -O0 -Wall Bad_Booking.cpp

