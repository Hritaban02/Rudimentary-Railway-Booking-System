	g++ -c -std=c++17 -g -O0 -Wall SeniorCitizen_Concession.cpp
	
Divyaang_Concession.o : Divyaang_Concession.cpp Divyaang_Concession.h
	g++ -c -std=c++17 -g -O0 -Wall Divyaang_Concession.cpp

Date.o : Date.cpp Date.h
	g++ -c -std=c++17 -g -O0 -Wall Date.cpp
	
Station.o : Station.cpp Station.h
	g++ -c -std=c++17 -g -O0 -Wall Station.cpp	

Name.o : Name.cpp Name.h 
	g++ -c -std=c++17 -g -O0 -Wall Name.cpp	
	
Gender.o : Gender.cpp Gender.h 
	g++ -c -std=c++17 -g -O0 -Wall Gender.cpp	
	
Divyaang.o : Divyaang.cpp Divyaang.h 
